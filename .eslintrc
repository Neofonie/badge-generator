{
  "extends": "standard",
  "plugins": [
    "html"
  ],
  "env": {
    "jasmine": true,
    "es6": true
  },
  "rules": {
    "space-before-function-paren": ["off"],
    "no-trailing-spaces": ["off"],
    "no-undef": ["off"],
    "no-unused-vars": ["off"],
    "comma-dangle": ["off"],
    "semi": ["off"]
  }
}
